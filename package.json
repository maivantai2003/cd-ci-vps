{
  "name": "ci-cd-vps",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:prod": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/Init -d ./src/data/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d ./src/data/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/data/data-source.ts",
    "migration:run:prod": "npm run typeorm:prod -- migration:run -d ./dist/data-source.js",
    "migration:revert:prod": "npm run typeorm:prod -- migration:revert -d ./dist/data-source.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/node": "^24.9.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  }
}
